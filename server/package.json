{
  "name": "@brokechef/server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && npx tsc-alias",
    "coverage": "vitest run --coverage",
    "dev": "tsx watch src",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "gen:types": "kysely-codegen --camel-case --dialect postgres --out-file src/database/types.ts && prettier --write src/database/types.ts",
    "db:embed": "tsx ./src/database/scripts/fillNonEmbeddedRecipes",
    "migrate:new": "tsx ./src/database/migrate/new",
    "migrate:latest": "tsx src/database/migrate/latest",
    "migrate:latest:prod": "npm run node-es src/database/migrate/latest.ts",
    "node-es": "node --import tsx",
    "prod": "npm run migrate:latest:prod && npm run start",
    "start": "npm run node-es src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@aws-sdk/types": "^3.862.0",
    "@eslint/js": "^9.32.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/busboy": "^1.5.4",
    "@types/chance": "^1.1.7",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/lodash-es": "^4.17.12",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.1.0",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "chance": "^1.1.13",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-unicorn": "^60.0.0",
    "jiti": "^2.5.1",
    "kysely-codegen": "^0.18.5",
    "prettier": "^3.6.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/lib-storage": "^3.966.0",
    "@aws-sdk/s3-request-presigner": "^3.879.0",
    "@google/genai": "^1.16.0",
    "@trpc/server": "^11.4.3",
    "better-auth": "^1.3.4",
    "bullmq": "^5.66.4",
    "busboy": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.8.2",
    "kysely": "^0.28.3",
    "lodash-es": "^4.17.21",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.5",
    "openai": "^6.15.0",
    "pg": "^8.16.3",
    "pg-error-enum": "^1.0.2",
    "pgvector": "^0.2.1",
    "sharp": "^0.34.3",
    "superjson": "^2.2.2",
    "supertest": "^7.1.4",
    "winston": "^3.17.0",
    "zod": "^4.0.14",
    "zod-validation-error": "^4.0.1"
  }
}
